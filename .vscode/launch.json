	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Run hello-world-javascript-action",
			"program": "${workspaceFolder}/hello-world-javascript-action/index.js",
			"envFile": "${workspaceFolder}/hello-world-javascript-action/.env",
			"console": "integratedTerminal",
			"preLaunchTask": "watch - hello-world-javascript-action"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Run dist hello-world-javascript-action",
			"program": "${workspaceFolder}/hello-world-javascript-action/dist/index.js",
			"envFile": "${workspaceFolder}/hello-world-javascript-action/.env",
			"console": "integratedTerminal",
			"preLaunchTask": "watch - hello-world-javascript-action"
		}
	]
}